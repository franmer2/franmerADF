{
	"name": "DelimitedTextToFileShare_parameters",
	"properties": {
		"description": "File Share with 3 parameters",
		"linkedServiceName": {
			"referenceName": "AzureFileStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Prefix": {
				"type": "string"
			},
			"Date": {
				"type": "string"
			},
			"Name": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Stored Procedure"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureFileStorageLocation",
				"fileName": {
					"value": "@concat(dataset().Prefix,'-',dataset().Date,'-',dataset().Name,'.csv')",
					"type": "Expression"
				},
				"folderPath": "ADFTest"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}